{
  "name": "website",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "bunx --bun next dev",
    "build": "bunx --bun next build",
    "postbuild": "bunx next-sitemap && mv public/sitemap.xml public/sitemap_index.xml && sed -i '' 's/sitemap-0/sitemap/g' public/sitemap_index.xml && mv public/sitemap-0.xml public/sitemap.xml",
    "start": "bunx --bun next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "ANALYZE=true bunx --bun next build",
    "prepare": "husky"
  },
  "dependencies": {
    "@arcjet/bun": "1.0.0-beta.8",
    "@arcjet/inspect": "1.0.0-beta.8",
    "@arcjet/next": "1.0.0-beta.8",
    "@next/bundle-analyzer": "^15.5.4",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.13",
    "@vercel/og": "^0.6.8",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "feed": "^5.1.0",
    "loops": "^5.0.1",
    "lucide-react": "^0.511.0",
    "next": "^16.0.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "next-view-transitions": "^0.3.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^22.18.6",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "8",
    "eslint-config-next": "^16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "string-replace-loader": "^3.2.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "@vercel/speed-insights"
    ]
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "@vercel/speed-insights",
    "esbuild",
    "sharp"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md,mdx}": [
      "prettier --write"
    ]
  }
}
